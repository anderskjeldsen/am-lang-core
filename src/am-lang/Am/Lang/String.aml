
namespace Am.Lang 
{
    native class String {
        native fun getLength(): Int
        native fun print()
        native op + (s: String): String
        native static fun fromBytes(bytes: UByte[], encoding: String): String
        native fun toBytes(encoding: String): UByte[]
        fun println() {
            var s = this + "\n"
            s.print()
        }
    }
}

