namespace Am.Lang 
{
    native class PropertyInfo {
        private var name: String
        private var index: UByte

        native fun getValue(target: Any)
        native fun setValue(target: Any, value: Any)
    }

    native class ClassRef 
    {
        private var className: String
        private var properties: PropertyInfo[]

        private native fun initFromAny(any: Any)

        override fun toString(): String {
            return this.className
        }
    }
}
