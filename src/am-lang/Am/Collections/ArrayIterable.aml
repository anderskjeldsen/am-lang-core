
namespace Am.Collections
{
    class ArrayIterable<T>(private var array: Array<T>) : Iterable<T> {
        import Am.Lang

        friend fun asIterable<T>(array: T[]): ArrayIterable<T> {
            return new ArrayIterable<T>(array)
        }

        fun iterator(): Iterator<T> {
            return new ArrayIterator<T>(this.array) as Iterator<T>
        }
    }
}