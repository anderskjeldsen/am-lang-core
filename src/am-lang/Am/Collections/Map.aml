namespace Am.Collections {

    interface MapEntry<K, V> {
        fun getKey(): K
        fun getValue(): V
    }

    interface Map<T, U> {
        import Am.Lang

        fun getSize(): Int
        fun get(key: T): U
        fun set(key: T, value: U)
        fun remove(key: T): U
        fun clear()
        fun containsKey(key: T): Bool
        fun containsValue(value: U): Bool
        fun isEmpty(): Bool
    }
}